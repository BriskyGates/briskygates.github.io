source "https://rubygems.org"

# GitHub Pages 使用的 Jekyll 版本
gem "github-pages", group: :jekyll_plugins

# 如果需要本地开发，可以取消下面的注释
# gem "jekyll", "~> 4.3"

# Windows 不支持 ffi gem 1.9.0，使用 1.8.11
gem "ffi", "~> 1.8.11"

platforms :mingw, :x64_mingw, :mswin, :jruby do
  gem "tzinfo", ">= 1", "< 3"
  gem "tzinfo-data"
end
